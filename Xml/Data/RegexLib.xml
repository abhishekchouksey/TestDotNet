<?xml version="1.0" encoding="utf-8" ?>
<regexLib>
  <regex name="Email" description="Validates any type of email address"  value="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*" />
  <regex name="Zip Code" description="Validates 00000 or 00000-0000" value="\d{5}(-\d{4})?" />
  <regex name="Phone Number" description="Validates 555-5555 or 555-555-55555" value="((\(\d{3}\) ?)|(\d{3}-))?\d{3}-\d{4}"/>
  <regex name="Ip Address" description="Validates 00.00.00.00" value="\b([0-9]{1,3})\.([0-9]{1,3})\.([0-9]{1,3})\.([0-9]{1,3})\b"/>
  <regex name="Numbers Only" description="Validates any Number" value="\b\d+\b"/>
</regexLib>